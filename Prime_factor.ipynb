    "\n",
    "def prime_factors(number):\n",
    "    lst=[]\n",
    "    i=2\n",
    "    while i<= number:\n",
    "        if number%i==0:\n",
    "            lst.append(i)\n",
    "            number=number/i\n",
    "        else:\n",
    "            i+=1\n",
    "    return lst"
   ]
